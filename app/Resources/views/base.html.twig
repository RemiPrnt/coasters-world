<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{% endblock %} - {{ 'website'|trans }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />

    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/vendor/bootstrap.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/vendor/bootstrap-responsive.min.css') }}">
    <!-- Font Awesome -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/vendor/font-awesome.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/vendor/font-awesome-social.css') }}">
    <!-- End Font Awesome -->
    <!-- Menu Styles -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/menu/core.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/menu/lwhite.css') }}">
    <!-- End Menu Styles -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/main.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/skins/orange.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/coastersworldsite/css/test.css') }}">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans">

    <script type="text/javascript" src="{{ asset('bundles/coastersworldsite/js/vendor/modernizr.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/coastersworldsite/js/vendor/jquery-1.9.1.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/coastersworldsite/js/vendor/bootstrap.min.js') }}"></script>

    {# stylesheets
        '../app/Resources/public/css/*' output='css/base.css' filter='?yui_css' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets #}

    {# javascripts
            '../app/Resources/public/js/thirdparty/jquery-1.8.3.min.js'
            '../app/Resources/public/js/thirdparty/*'
            '../app/Resources/public/js/*' output='js/base.js' filter='?yui_js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts #}

    {% block stylesheets %}{% endblock %}

</head>
<body class="wide">
    <div class="layout">
        {% include '::header.html.twig' %}
        <div class="main">
            <div class="container">
                {% block body %}{% endblock %}
            </div>
        </div>
        {% include '::footer.html.twig' %}
    </div>
    {% block javascripts %}{% endblock %}
    <script type="text/javascript" src="{{ asset('bundles/coastersworldsite/js/plugins.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/coastersworldsite/js/main.js') }}"></script>
</body>
</html>
